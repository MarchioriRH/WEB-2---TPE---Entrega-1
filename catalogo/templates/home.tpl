{* template creado para mostrar el carrusel de imagenes del home *}
{include file="header.tpl"}
    <div class="container">
        <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="./img/catalogo/aprilia-rsv4-factory-2021-09 682X384.jpeg" class="d-block w-100" alt="Aprilia RSV4">
                        <div class="carousel-caption d-none d-sm-block">
                            <h5>Aprilia RSV4</h5>
                            <p>0 km - Fecha de ingreso al pais a confirmar.</p>
                        </div>
                </div>
                <div class="carousel-item">
                    <img src="./img/catalogo/Ford_F-150_Raptor_2021_600x344.jpg" class="d-block w-100" alt="F150 Raptor">
                        <div class="carousel-caption d-none d-sm-block">
                            <h5>F150 Raptor</h5>
                            <p>0 km - Modelo 2021 - Disponible para entrega inmediata.</p>
                        </div>
                </div>
                <div class="carousel-item">
                    <img src="./img/catalogo/2011LamborghiniAventador_497x227.jpg" class="d-block w-100" alt="Lamborghini Aventador 2011">
                        <div class="carousel-caption d-none d-sm-block">
                            <h5>Lamborghini Aventador</h5>
                            <p>1245 kms - Modelo 2011 - Disponible para entrega inmediata</p>
                        </div>
                </div>
                <div class="carousel-item">
                    <img src="./img/catalogo/Ferrari-812 682X384.jpg" class="d-block w-100" alt="Ferrari 812">
                        <div class="carousel-caption d-none d-sm-block">
                            <h5>Ferrari 812</h5>
                            <p>45 kms - Modelo 2020 - Disponible para entrega inmediata</p>
                        </div>
                </div>
                 <div class="carousel-item">
                    <img src="./img/catalogo/porsche 911 682x383.jpeg" class="d-block w-100" alt="Porsche 911">
                        <div class="carousel-caption d-none d-sm-block">
                            <h5>Porsche 911</h5>
                            <p>22365 kms - Modelo 2019 - Disponible para entrega inmediata</p>
                        </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>
{include file="footer.tpl"}